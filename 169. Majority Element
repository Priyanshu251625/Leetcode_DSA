class Solution {
    public int majorityElement(int[] nums) {
        int n=nums.length;
        int i=0;
        int count=0,max_el=-1;
        while(i<n){
            if(count==0){
                max_el=nums[i];
                count++;
                i++;
            }
            else if(count!=0&&nums[i]==max_el){
                count++;
                i++;
            }
            else if(count!=0&&nums[i]!=max_el){
                count--;
                i++;
            }
        }
        int cnt=0;
        for(int j=0;j<n;j++){
            if(nums[j]==max_el){
                cnt++;
            }
        }
        if(cnt>n/2){
            return max_el;
        }
        return -1;
    }
}
