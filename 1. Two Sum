class Solution {
    public int[] twoSum(int[] nums, int target) {
        HashMap<Integer,Integer>Sums=new HashMap<>();
        int n=nums.length;
        int ind[]={-1,-1};
        for(int i=0;i<n;i++){
            int rem=target-nums[i];
            if(Sums.containsKey(rem)){
                ind=new int[]{Sums.get(rem),i};
                return ind;
            }
            else{
                Sums.put(nums[i],i);
            }
        }
        return ind;
    }
}
