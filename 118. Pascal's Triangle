class Solution {
    public List<List<Integer>> generate(int numRows) {
        List<List<Integer>>ans=new ArrayList<>();
        for(int row=1;row<=numRows;row++){
            ArrayList<Integer>temp=new ArrayList<>();
            int term =1;
            temp.add(term);
            for(int col=1;col<row;col++){
                term=term*(row-col);
                term=term/col;
                temp.add(term);
            }
            ans.add(temp);
        }
        return ans;
    }
}
