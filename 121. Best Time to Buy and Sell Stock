class Solution {
    public int maxProfit(int[] prices) {
        int min_cost=prices[0];
        int n=prices.length;
        int profit=0;
        for(int i=1;i<n;i++){
            int prof=prices[i]-min_cost;
            profit=Math.max(profit,prof);
            min_cost=Math.min(min_cost,prices[i]);
        }
        return profit;
    }
}
