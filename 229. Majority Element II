class Solution {
    public List<Integer> majorityElement(int[] nums) {
        List<Integer>ans=new ArrayList<>();
        int count1=0,count2=0;
        int num1=Integer.MIN_VALUE,num2=Integer.MIN_VALUE;
        int n=nums.length;
        for(int i=0;i<n;i++){
            if(count1==0&&nums[i]!=num2){
                count1=1;
                num1=nums[i];
            }
            else if(count2==0 && nums[i]!=num1){
                count2=1;
                num2=nums[i];
            }
            else if(nums[i]==num1){
                count1++;
            }
            else if(nums[i]==num2){
                count2++;
            }
            else{
                count1--;
                count2--;
            }
        }
        int cnt1=0,cnt2=0;
        for(int i=0;i<n;i++){
            if(nums[i]==num1){
                cnt1++;
            }
            else if(nums[i]==num2){
                cnt2++;
            }
        }
        if(cnt1>n/3){
            ans.add(num1);
        }
        if(cnt2>n/3){
            ans.add(num2);
        }
        return ans;
    }
}
