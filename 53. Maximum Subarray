class Solution {
    public int maxSubArray(int[] nums) {
        int n=nums.length;
        int sum=0;
        int max_Sum=Integer.MIN_VALUE;
        for(int i=0;i<n;i++){
            sum+=nums[i];
            if(sum>max_Sum){
                max_Sum=sum;
            }
            if(sum<0){
                sum=0;
            }
        }
        return max_Sum;
    }
}
