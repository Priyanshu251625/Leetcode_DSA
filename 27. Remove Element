class Solution {
    public int removeElement(int[] nums, int val) {
        int n=nums.length;
        if(n==0){
            return 0;
        }
        int i=0,j=0;
        while(i<n&&nums[i]!=val){
            i++;
        }
        j=i+1;
        while(j<n){
            if(nums[j]==val){
                j++;
            }
            else{
                nums[i]=nums[j];
                nums[j]=val;
                i++;
            }
        }
        return i;
    }
}
