class Solution {
    public int subarraySum(int[] nums, int k) {
        HashMap<Integer,Integer>numMap=new HashMap<>();
        int n=nums.length;
        int count=0;
        int sum=0;
        for(int i=0;i<n;i++){
            sum+=nums[i];
            if(sum==k){
                count++;
            }
            int rem=sum-k;
            if(numMap.containsKey(rem)){
                count+=numMap.get(rem);
            }
            if(numMap.containsKey(sum)){
                numMap.put(sum,numMap.get(sum)+1);
            }
            else{
                numMap.put(sum,1);
            }
        }
        return count;
    }
}
