import java.util.*;
class Solution {
    public boolean isPalindrome(String s) {
        s =s.toLowerCase().replaceAll("[^a-zA-Z0-9]","");
        StringBuilder s1=new StringBuilder(s);
        s1.reverse();
        String s2=s1.toString();
        if(s2.equals(s)){
            return true;
        }
        return false;
    }
}
